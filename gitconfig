  name = Tee Parham
  email = tee@neighborland.com

[core]
  excludesfile = /Users/tee/.gitignore
  whitespace = -trailing-space
  ignorecase = false

[github]
  user = teeparham
  token = <.replace github-token>

[credential]
  helper = osxkeychain

