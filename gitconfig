[color]
  ui = auto

[color "diff"]
  meta = blue bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "branch"]
  current = yellow reverse
  local = yellow bold
  remote = green bold
  plain = red bold

[color "status"]
  added = yellow
  changed = green bold
  untracked = blue bold

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  sb = show-branch
  df = diff --ignore-space-change
  aa = add -A

[branch]
  autosetuprebase = always

[push]
  # push current branch even if you've never pushed it before
  default = current

